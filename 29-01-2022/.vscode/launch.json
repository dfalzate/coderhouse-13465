  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Mem",
      "type": "pwa-node",
      "program": "${workspaceFolder}/oakMem/src/server.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    },
    {
      "request": "launch",
      "name": "Mongo",
      "type": "pwa-node",
      "program": "${workspaceFolder}/oakMongo/src/server.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "--allow-all"
      ],
      "attachSimplePort": 9229
    }
  ]
}